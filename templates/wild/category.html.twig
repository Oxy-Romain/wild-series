{# templates/wild/category.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}{{ category.name }}{% endblock %}

{% block body %}
    <div class="media">
        <div class="media-body">
            {%  for program in programs %}
                <h1 class="mt-0">{{ program.title }}</h1>
                <p>{{ program.summary }}</p>
                <p>Categorie : {{ program.category.name }}</p>
            {% endfor %}
        </div>
    </div>
{% endblock %}